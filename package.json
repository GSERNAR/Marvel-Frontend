{
  "name": "marvel_frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://ec2-3-134-88-175.us-east-2.compute.amazonaws.com/",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:js": "react-app-rewired start",
    "start:css": "npx tailwindcss -i ./tailwind.css -o ./src/index.css --jit -w",
    "start:lang":"nodemon --watch src --ext json --exec 'node language-extractor.js'",
    "-start:lint": "nodemon --watch src --ext js,jsx --exec 'concurrently npm:lint:*'",
    "lint:eslint": "eslint --fix 'src/**/*.{js,jsx}'",
    "lint:prettier": "npx prettier 'src/**/*.{js,jsx}' --write --log-level silent",
    "build": "react-app-rewired build && npm run start:css && node language-extractor.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 opera version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.2.3",
    "bootstrap": "^5.1.3",
    "clsx": "^1.2.1",
    "fs": "^0.0.1-security",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.1",
    "jwt-decode": "^4.0.0",
    "ldrs": "^1.0.2",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-icons": "^4.7.1",
    "react-intl": "^5.24.6",
    "react-lazy-load": "^4.0.1",
    "react-reveal": "^1.2.2",
    "react-reveal-responsive-carousel": "^1.0.0",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.1",
    "react-scroll-to-component": "^1.0.2",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^7.6.0",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.34.1",
    "nodemon": "^3.1.3",
    "prettier": "^3.2.5",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.2.7"
  }
}
